{
    "version": "0.2.0",
    "configurations": [
        {
            "name": "Launch Package",
            "type": "go",
            "request": "launch",
            "mode": "auto",
            "program": "${workspaceFolder}/main.go",
            "envFile": "${workspaceFolder}/.env",
            "stopOnEntry": false,
            "showOutput": "auto",
            "cwd": "${workspaceFolder}",
            "externalConsole": false,
            "console": "integratedTerminal"
        }
    ]
}